{
  "name": "mission-control",
  "version": "1.0.0",
  "description": "OpenClaw Agent System Mission Control Dashboard",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:data && next build && npm run copy:data",
    "start": "next start",
    "build:data": "tsx scripts/build-data.ts",
    "copy:data": "tsx scripts/copy-data.ts",
    "watch:data": "tsx scripts/watch-data.ts",
    "dev:full": "concurrently \"npm run watch:data\" \"npm run dev\"",
    "lint": "next lint",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "chokidar": "^3.6.0",
    "lucide-react": "^0.574.0",
    "next": "^15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "yaml": "^2.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.0",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  }
}
